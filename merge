      "source": [
        "import pandas as pd  \n",
        "a = pd.DataFrame({  \n",
        "   'id':[1,2,3,4],  \n",
        "   'Name': ['John', 'Parker', 'Smith', 'Parker'],  \n",
        "   'subject_id':['sub1','sub2','sub4','sub6']})  \n",
        "b= pd.DataFrame({  \n",
        "    'id':[1,2,3,4],  \n",
        "   'Name': ['William', 'Albert', 'Tony', 'Allen'],  \n",
        "   'subject_id':['sub2','sub4','sub3','sub6']})  \n",
        "a\n",
        "b\n",
        "\n",
        "\n"
      ]
    },
